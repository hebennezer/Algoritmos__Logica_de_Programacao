Algoritmo "teste_repite_ate"

Var

   C, F : real
   resp : caractere


Inicio

   repita
      escreva("Digite a temperatura em Celsius: ")
      leia(C)

      F <- (9 * c) / 5 + 32
      escreval("Equivalente em Fahrenheit: ", F:3:1)

      escreval("Deseja repetir (s/n)? ")
      leia(resp)
   ate resp <> "s"
   
   
   


Fimalgoritmo