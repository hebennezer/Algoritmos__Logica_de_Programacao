Algoritmo "consumo"

Var

   distanciaPerc, combustivelGas, avg : real

Inicio

   escreva("Distancia percorrida: ")
   leia(distanciaPerc)
   escreva("Combustivel gasto: ")
   leia(combustivelGas)
   
   avg <- distanciaPerc / combustivelGas
   
   escreval("Consumo medio = ", avg:5:3)

Fimalgoritmo